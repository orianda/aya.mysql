{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-empty-interface": "off"
  },
  "overrides": [
    {
      "files": [
        "**/*.spec.ts"
      ],
      "rules": {
        "@typescript-eslint/no-unused-vars": [
          "warn",
          {
            "args": "none"
          }
        ]
      }
    }
  ]
}
